pipeline{
  agent any
  environment{
  PATH="/home/ubuntu/opt/apache-maven-3.6.3/bin:$PATH
  }
  
  stages{
    stage("Git Checkout"){
      steps{
        git 'https://github.com/maheshdte/ValaxyTech-hello-world'
        }
        
      }
     stage("Maven Build"){
       steps{
         sh "mvn clean package"
       }
     
     }
        
  }
        
}
